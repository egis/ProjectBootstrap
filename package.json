{
  "name": "BootstrappedIronMan",
  "moduleName": "BootstrappedIronMan",
  "port": 8120,
  "mainFile": "BootstrappedIronMan",
  "plugin": "PortalApp",
  "build": {
    "autoImportAll": false
  },
  "scripts": {
    "dev": "rm -rf dist && gulp --gulpfile node_modules/@egis/build-tools/Gulpfile.js --cwd ./ --srcDir src/js plugin_watch --watch",
    "install-ci-tools": "npm install -g @egis/ci-tools",
    "setup": "yarn --frozen-lockfile",
    "test": "echo ok",
    "build": "rm -rf dist/main; rm -rf build/ && gulp --gulpfile node_modules/@egis/build-tools/Gulpfile.js --cwd ./ --env production --srcDir src/js plugin",
    "update:windows": "sh node_modules/@egis/build-tools/npm-install.sh",
    "full": "yarn setup && yarn build && yarn test"
  },
  "devDependencies": { 
    "@egis/build-tools": "^5.0.0-pre.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/egis/BootstrappedIronMan.git"
  },
  "version": "1.0.0"
}
